    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@tinymce/tinymce-angular": "^1.0.5",
    "@types/file-saver": "0.0.1",
    "@types/moment-timezone": "^0.5.2",
    "angular-confirmation-popover": "^3.2.0",
    "angular-io-datepicker": "^1.3.1",
    "angular-tinymce": "^3.1.5",
    "angular2-busy": "^2.0.4",
    "angular2-csv": "^0.2.5",
    "angular2-datatable": "^0.6.0",
    "angular2-serverpagination-datatable": "^1.5.4",
    "angular2-social-login": "^3.1.1",
    "angular2-tinymce": "^2.1.2",
    "angular4-color-picker": "^1.4.2",
    "ap-angular2-fullcalendar": "^1.3.2",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "file-saver": "^1.3.3",
    "lodash": "^4.17.4",
    "ng-pdf-make": "0.0.2",
    "ng-pick-datetime": "^5.0.0-beta.4",
    "ng2-bootstrap-modal": "^1.0.1",
    "ng2-datepicker-bootstrap": "^1.0.0",
    "ng2-datetime": "^1.4.0",
